# Google OAuth Client ID (from Google Cloud Console)
CLIENT_ID=your_client_id_here

# Redirect URI for OAuth flow (default for Chrome extensions)
REDIRECT_URI=https://your_extension_id.chromiumapp.org/

# Gmail topic name for push notifications
GMAIL_TOPIC_NAME=projects/your_project_id/topics/gmail_notifications

# Local development server URL
LOCAL_SERVER_URL=http://localhost:8080
